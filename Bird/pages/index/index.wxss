page {
    background: #FAFAFA;
    overflow: hidden;
}
.time{
    vertical-align: middle;
    text-align: center;
  }
  .contentImg{
    vertical-align: top;
    text-align: center;
    width: 100%;
    height: 70%;
    margin-left: 0rpx;
  }
  
  .content{
    margin-top: 5rpx;
  }
  
  .card{
    background: #FFFFFF;
    text-align: center;
    vertical-align: middle;
    width: 90%;
    height: 800rpx;
    margin-top: 100rpx;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 50rpx;
    padding: 6rpx;
    box-shadow: 0rpx 0.5rpx rgba(0,0,0, .3);
  }
  
  .albumText{
    color:#708090;
    font-size: 35rpx;
    vertical-align: middle;
    text-align: center;
    margin-bottom: 6rpx;
  }
  
  .btn {
    font-size: 40rpx;
    vertical-align: middle;
    text-align: center;
    line-height: 100rpx;
    color: #FFFFFF;
    width: 350rpx;
    height: 5%;
    border-radius: 400rpx;
    background-color:#778899;
  }
  
  .btn .icon{
    /* line-height: 300rpx; */
    margin-right: 20rpx;
  }